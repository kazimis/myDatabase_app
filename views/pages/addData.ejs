<!DOCTYPE html>
<html>
<head>
  <link rel="stylesheet" href="stylesheets/myDataBase.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
    <h1>Add Data</h1>
    <div class="page_nav">
      <a href="/myDataBase">Home</a>
    </div>
    <div class="box_addData">
      <form id = "myForm" action="/add" method="POST">
        <div class="label_input">
          <label for="name">Tokimon Name</label>
  				<input type="text" name="1" placeholder="Name">
        </div>
        <div class="label_input">
          <label for="weight">Weight</label>
          <input type="text" name="2" placeholder="weight">
        </div>
        <div class="label_input">
          <label for="height">Height</label>
          <input type="text" name="3" placeholder="height">
        </div>
        <div class="label_input">
          <label for="fly">Fly</label>
          <input type="text" name="4" placeholder="fly">
        </div>
        <div class="label_input">
          <label for="fight">Fight</label>
          <input type="text" name="5" placeholder="fight">
        </div>
        <div class="label_input">
          <label for="fire">Fire</label>
          <input type="text" name="6" placeholder="fire">
        </div>
        <div class="label_input">
          <label for="water">Water</label>
          <input type="text" name="7" placeholder="water">
        </div>
        <div class="label_input">
          <label for="electric">Electric</label>
          <input type="text" name="8" placeholder="electric">
        </div>
        <div class="label_input">
          <label for="frozen">Frozen</label>
          <input type="text" name="9" placeholder="frozen">
        </div>

        <div class="label_input">
          <label for="Trainer_Name">Trainer Name</label>
          <input type="text" name="10" placeholder="Trainer_Name">
        </div>
        <div class="label_input">
          <input class = "submit_button" name = "submit" type="submit" onclick="return ifEmpty()">
        </div>

			</form>
    </div>
</body>
<script src="myDataBase.js"></script>
</html>
